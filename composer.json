{
    "name": "t3g/intercept",
    "description": "",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Susanne Moog",
            "email": "susanne.moog@typo3.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0",
        "ext-SimpleXML": "*",
        "ext-bcmath": "*",
        "ext-json": "*",
        "composer/semver": "^3.2",
        "doctrine/annotations": "^1.6",
        "doctrine/collections": "^1.5",
        "doctrine/dbal": "^2.9",
        "doctrine/doctrine-bundle": "^2.3.0",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/migrations": "^3.1.1",
        "doctrine/orm": "^2.6",
        "doctrine/persistence": "^2.1",
        "dragonmantank/cron-expression": "^2.3",
        "eightpoints/guzzle-bundle": "^v8.2.0",
        "guzzlehttp/guzzle": "^6.5.8",
        "kbsali/redmine-api": "~1.0",
        "knplabs/knp-menu": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.0",
        "php-amqplib/php-amqplib": "^2.8",
        "psr/http-message": "^1",
        "psr/log": "^1",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.2",
        "sentry/sentry-symfony": "^4.1",
        "setono/cron-expression-bundle": "^1.0",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/doctrine-bridge": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/monolog-bundle": "^3.3",
        "symfony/options-resolver": "^5.0",
        "symfony/process": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/security-core": "^5.0",
        "symfony/security-csrf": "^5.0",
        "symfony/security-http": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^5.0",
        "symfony/translation-contracts": "^2.3",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^5.0",
        "symplify/git-wrapper": "^9.2",
        "t3g/symfony-keycloak-bundle": "@dev",
        "t3g/symfony-template-bundle": "^2.16",
        "t3g/symfony-usercentrics-bundle": "^1.0.0",
        "twig/twig": "^v3.3.0",
        "woeler/phpdiscord": "^1.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "fakerphp/faker": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^0.12.22",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/maker-bundle": "^1.10",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.3",
        "symfony/var-dumper": "^4.2",
        "symfony/web-profiler-bundle": "^5.3"
    },
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist",
            "t3g/symfony-template-bundle": "source"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "t3g:test:php:unit": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml --testsuite \"Unit Test Suite\""
        ],
        "t3g:test:php:functional": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml --testsuite \"Functional Test Suite\""
        ],
        "t3g:test:php:cover": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml  --coverage-clover var/phpunit/coverage.xml --coverage-html var/phpunit/coverage/"
        ],
        "t3g:test": [
            "@t3g:test:php:unit"
        ],
        "t3g:cgl": [
            "php-cs-fixer fix --config build/.php-cs-fixer.dist.php -v --dry-run"
        ],
        "t3g:cgl:fix": [
            "php-cs-fixer fix --config build/.php-cs-fixer.dist.php"
        ],
        "t3g:phpstan": [
            "phpstan analyse src/ -c build/phpstan.neon"
        ]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/typo3gmbh/"}
    ]
}
