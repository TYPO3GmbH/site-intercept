{
    "name": "t3g/intercept",
    "description": "",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Susanne Moog",
            "email": "susanne.moog@typo3.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-SimpleXML": "*",
        "ext-bcmath": "*",
        "ext-json": "*",
        "composer/semver": "^3.4.3",
        "doctrine/annotations": "^2.0.2",
        "doctrine/dbal": "^4.2.4",
        "doctrine/doctrine-bundle": "^2.14.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/migrations": "^3.9",
        "doctrine/orm": "^3.4",
        "gitonomy/gitlib": "dev-bugfix/205",
        "guzzlehttp/guzzle": "^7.9.3",
        "kbsali/redmine-api": "^2.8",
        "knplabs/knp-menu": "^3.8",
        "knplabs/knp-paginator-bundle": "^5.9",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/log": "^2.0",
        "ramsey/uuid": "^4.8.1",
        "sentry/sentry-symfony": "^4.14",
        "symfony/config": "^6.4.22",
        "symfony/console": "^6.4.22",
        "symfony/dependency-injection": "^6.4.22",
        "symfony/doctrine-bridge": "^6.4.22",
        "symfony/dotenv": "^6.4.16",
        "symfony/expression-language": "^6.4.13",
        "symfony/filesystem": "^6.4.13",
        "symfony/finder": "^6.4.17",
        "symfony/flex": "^2.7.1",
        "symfony/form": "^6.4.22",
        "symfony/framework-bundle": "^6.4.22",
        "symfony/http-foundation": "^6.4.22",
        "symfony/http-kernel": "^6.4.22",
        "symfony/mailer": "^6.4.21",
        "symfony/monolog-bundle": "^3.10",
        "symfony/options-resolver": "^6.4.16",
        "symfony/process": "^6.4.20",
        "symfony/routing": "^6.4.22",
        "symfony/runtime": "^6.4.22",
        "symfony/security-bundle": "^6.4.22",
        "symfony/security-core": "^6.4.22",
        "symfony/security-csrf": "^6.4.13",
        "symfony/security-http": "^6.4.22",
        "symfony/serializer": "^6.4.22",
        "symfony/twig-bundle": "^6.4.13",
        "symfony/validator": "^6.4.22",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "^6.4.21",
        "t3g/datahub-api-library": "^3.5.3",
        "t3g/symfony-datahub-bundle": "^3.0.1",
        "t3g/symfony-keycloak-bundle": "^3.0.4",
        "t3g/symfony-template-bundle": "^4.0.8",
        "t3g/symfony-usercentrics-bundle": "^2.0.0",
        "twig/twig": "^3.21.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.7.1",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.17",
        "phpstan/phpstan-doctrine": "^2.0.3",
        "phpstan/phpstan-symfony": "^2.0.6",
        "phpunit/phpunit": "^11.5.23",
        "rector/rector": "^2.0.18",
        "symfony/browser-kit": "^6.4.19",
        "symfony/css-selector": "^6.4.13",
        "symfony/maker-bundle": "^1.63",
        "symfony/phpunit-bridge": "^6.4.16",
        "symfony/stopwatch": "^6.4.19",
        "symfony/var-dumper": "^6.4.21",
        "symfony/web-profiler-bundle": "^6.4.19",
        "t3g/lib-test-helper": "dev-main"
    },
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist",
            "t3g/symfony-template-bundle": "source"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "t3g:test:php:unit": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml --testsuite \"Unit Test Suite\""
        ],
        "t3g:test:php:functional": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml --testsuite \"Functional Test Suite\""
        ],
        "t3g:test:php:cover": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml  --coverage-clover var/phpunit/coverage.xml --coverage-html var/phpunit/coverage/"
        ],
        "t3g:test": [
            "@t3g:test:php:unit"
        ],
        "t3g:cgl": [
            "php-cs-fixer fix --config build/.php-cs-fixer.dist.php -v --dry-run"
        ],
        "t3g:cgl:fix": [
            "php-cs-fixer fix --config build/.php-cs-fixer.dist.php"
        ],
        "t3g:phpstan": [
            "phpstan analyse src/ -c build/phpstan.neon"
        ],
        "t3g:phpstan:create-baseline": [
            "phpstan analyze src/ -c build/phpstan.neon --generate-baseline build/phpstan-baseline.neon"
        ]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "repositories": {
        "gitlib": {
            "type": "vcs",
            "url": "https://github.com/andreasfernandez/gitlib.git"
        },
        "0": {
            "type": "composer",
            "url": "https://repo.packagist.com/typo3gmbh/"
        }
    },
    "type": "project"
}
