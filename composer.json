{
    "name": "t3g/intercept",
    "description": "",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Susanne Moog",
            "email": "susanne.moog@typo3.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0",
        "ext-SimpleXML": "*",
        "ext-bcmath": "*",
        "ext-json": "*",
        "composer/semver": "^1.5",
        "cpliakas/git-wrapper": "^2.1",
        "doctrine/annotations": "^1.6",
        "doctrine/collections": "^1.5",
        "doctrine/dbal": "^2.9",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/migrations": "^2.0",
        "doctrine/orm": "^2.6",
        "doctrine/persistence": "^1.1",
        "dragonmantank/cron-expression": "^2.3",
        "eightpoints/guzzle-bundle": "^7.4",
        "graylog2/gelf-php": "^1.6",
        "guzzlehttp/guzzle": "^6.3",
        "kbsali/redmine-api": "~1.0",
        "knplabs/knp-menu": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.0",
        "php-amqplib/php-amqplib": "^2.8",
        "psr/http-message": "^1",
        "psr/log": "^1",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.2",
        "setono/cron-expression-bundle": "^1.0",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/config": "^4.3",
        "symfony/console": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/doctrine-bridge": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/filesystem": "^4.2",
        "symfony/finder": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/ldap": "^4.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/options-resolver": "^4.2",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/security-core": "^4.2",
        "symfony/security-csrf": "^4.2",
        "symfony/security-http": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.2",
        "symfony/translation-contracts": "^1.1",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^4.2",
        "t3g/symfony-template-bundle": "^2.3",
        "t3g/symfony-usercentrics-bundle": "^1.0",
        "twig/twig": "^3.0",
        "woeler/phpdiscord": "^1.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.12",
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^7.4",
        "rector/rector": "^0.7.19",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/maker-bundle": "^1.10",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.2"
    },
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist",
            "t3g/symfony-template-bundle": "source"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "t3g:test:php:unit": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml --testsuite \"Unit Test Suite\""
        ],
        "t3g:test:php:functional": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml --testsuite \"Functional Test Suite\""
        ],
        "t3g:test:php:cover": [
            "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml  --coverage-clover var/phpunit/coverage.xml --coverage-html var/phpunit/coverage/"
        ],
        "t3g:test": [
            "@t3g:test:php:unit"
        ],
        "t3g:cgl": [
            "mkdir -p var/php-cs-fixer/; php-cs-fixer fix --config build/.php_cs.dist --format=junit > var/php-cs-fixer/php-cs-fixer.xml"
        ]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
