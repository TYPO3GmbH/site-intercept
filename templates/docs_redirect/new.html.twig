{% extends 'layout.html.twig' %}
{% import 'macros/macros.html.twig' as macros %}
{% block title %}Create new Documentation Redirect{% endblock %}
{% block headline %}Create new Documentation Redirect{% endblock %}
{% block body %}

    {% frame %}
        <div class="card">
            <div class="card-body">
                {{ include('docs_redirect/_form.html.twig') }}
            </div>
        </div>
        <div class="row mt-4 justify-content-between">
            <div class="col-auto">
                {{
                    macros.linkbutton(
                        path('admin_redirect_index'),
                        'back to list',
                        'outline-secondary',
                        'arrow-left'
                    )
                }}
            </div>
        </div>
    {% endframe %}

{% endblock %}
