{% extends 'layout.html.twig' %}
{% block title %}Documentation deployments{% endblock %}
{% block headline %}Deployed documentation{% endblock %}
{% block body %}

    {% frame %}
        <h2>Add Deployed documentation projects</h2>
        <div class="card">
            <div class="card-body">
                {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <div class="text-center">
                        {% if (showPrev) %}
                        <button class="btn btn-outline-secondary" type="button" onclick="history.back();">
                            {{ icon('actions-arrow-left') }}
                            Previous
                        </button>
                        {% endif %}
                        <button class="btn btn-primary pull-right">
                            {{ icon('actions-arrow-right') }}
                            Next
                        </button>
                    </div>
                {{ form_end(form) }}
            </div>
        </div>
    {% endframe %}

{% endblock %}
